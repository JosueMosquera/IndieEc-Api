<link rel="stylesheet" href="/css/catalogueList/catalogueList.css">
<head>
    <title>Catalogos</title>
</head>

<body>
    <div id="wrap">
        <h1 id="catalogue-title">Catalogo de artistas</h1>
        <div id="store-wrapper">
            <section id="catalogue-list">
                 {{#each catalogues}}
                <div id="catalogue-item">
                <div id="catalogue-container">
                <div id="card">
                  <img src="https://promocionmusical.es/wp-content/uploads/2018/05/crossroads-MSG-merch-stand-2-810x554.jpg" id="card-img-top" alt="product-image">
                    <div id="card-body">
                    <h3 id="card-title">{{name}}</h5>
                    <a href="/artist-catalogue/{{id}}" id="btn-primary">Ir al catalogo</a>
                    </div>
                </div>
                   </div>
            </div>
            {{/each}}
            </section>
        </div>
    </div>
</body>